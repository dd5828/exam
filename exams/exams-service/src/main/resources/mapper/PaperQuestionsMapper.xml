<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.examsservice.mapper.PaperQuestionsMapper">

    <select id="getById" resultType="com.exam.entity.PaperQuestions">
        select * from paper_questions
        where paper_id=#{id} and question_id=#{questionId}
    </select>


</mapper>
